{{#if issueUnmap}}
  {{#unless denyUnmap}}
    {{#is page_instance.type "Issue"}}
      <issue-unmap
        {issue-instance}="page_instance"
        {target}="instance">
          <li class="border">
            <span class="unmap-link">
              <i class="fa fa-ban"></i>Unmap
            </span>
          </li>
      </issue-unmap>
    {{else}}
      {{#if visibleIssueUnmap}}
        <issue-unmap
          {issue-instance}="instance"
          {target}="page_instance">
            <li class="border">
              <span class="unmap-link">
                <i class="fa fa-ban"></i>Unmap
              </span>
            </li>
        </issue-unmap>
      {{/if}}
    {{/is}}
  {{/unless}}
{{else}}
  <content></content>
{{/if}}
