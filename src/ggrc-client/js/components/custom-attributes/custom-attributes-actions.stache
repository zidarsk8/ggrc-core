{{#unless formEditMode}}
<confirm-edit-action
  on:setInProgress="edit()"
  instance:from="instance">
  <button type="button"
          class="btn btn-small btn-gray custom-attributes-actions__edit-button"
          {{#if disabled}}disabled{{/if}}
          on:el:click="confirmEdit()">
    Edit Answers
  </button>
</confirm-edit-action>
{{/unless}}
