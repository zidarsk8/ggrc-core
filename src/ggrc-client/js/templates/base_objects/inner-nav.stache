{{#if isAuditScope}}
  <audit-inner-nav
    instance:from="instance"
    widgetDescriptors:from="innerNavDescriptors"
    on:activeChanged="showWidgetArea(%event)"
  ></audit-inner-nav>
{{else}}
  <inner-nav
    instance:from="instance"
    widgetDescriptors:from="innerNavDescriptors"
    on:activeChanged="showWidgetArea(%event)"
  ></inner-nav>
{{/if}}
