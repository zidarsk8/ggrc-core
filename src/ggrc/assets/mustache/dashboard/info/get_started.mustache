<ul class="quick-list">
  {{#start_menu}}
    {{#is_allowed 'create' model_name context=null}}
    <li>
      <a href="javascript://" data-toggle="modal-ajax-form" data-modal-class="modal-wide"
          data-object-singular="{{model_name}}" data-object-plural="{{model_plural}}"
          data-modal-reset="reset" data-form-target="redirect">
        <span class="icon-wrap">
          <i class="grcicon-{{model_lowercase}}-color"></i>
        </span>
        {{display_name}}
      </a>
    </li>
    {{/is_allowed}}
  {{/start_menu}}
  {{#any_allowed 'create' object_menu context=null}}
    <li class="top-space hidden-widgets-list">
      <a href="#" class="dropdown-toggle" data-toggle="dropdown">
        <span class="icon-wrap">
          <i class="fa fa-plus-circle white"></i>
        </span>
        Create new object
      </a>
      <div class="dropdown-menu" role="menu">
        {{#object_menu}}
          {{#is_allowed 'create' model_name context=null}}
          <div class="inner-nav-item">
            <a href="javascript://" data-toggle="modal-ajax-form" data-modal-class="modal-wide"
            data-object-singular="{{model_name}}" data-object-plural="{{model_plural}}"
            data-modal-reset="reset" data-form-target="redirect">
              <div class="oneline">
                <i class="grcicon-{{model_lowercase}}-color"></i>
                {{display_name}}
              </div>
            </a>
          </div>
          {{/is_allowed}}
        {{/object_menu}}
      </div>
    </li>
  {{/any_allowed}}

  <li class="top-space">
    {{! <a href="javascript://" data-toggle="unified-search" data-original-title="search items" data-join-object-type="MultitypeSearch"> }}
    <a href="/objectBrowser">
      <span class="icon-wrap">
        <i class="grcicon-all-object-white"></i>
      </span>
      All Objects
    </a>
  </li>
</ul><!-- tree-structure end -->
