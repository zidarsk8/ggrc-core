<div class="preview-container">
{{#mixed_content_check url}}
  <iframe class="preview" src="{{schemed_url url}}" sandbox="allow-same-origin allow-scripts allow-forms"></iframe>
{{else}}
  <div style="display:table-cell">The linked content is insecure and cannot be shown inside a secure application</div>
{{/mixed_content_check}}
</div>
