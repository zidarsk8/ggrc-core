{{!div class="modal-footer"}}
  <div class="row-fluid">
    <div class="span3">
      <div class="deny-buttons">
        <a class="btn btn-small btn-red preventdoubleclick" data-toggle="confirm" href="javascript://">{{firstnonempty modal_confirm 'Confirm'}}</a>
      </div>
    </div>
    <div class="span6">
      &nbsp;
    </div>
    <div class="span3">
      <div class="confirm-buttons">
        <a class="btn btn-small btn-green" data-dismiss="modal" href="javascript://">Cancel</a>
      </div>
    </div>
  </div>
{{!/div}}
