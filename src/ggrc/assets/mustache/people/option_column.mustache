<div class="search-title">
  <div class="row-fluid">
    <div class="span6">
      <h4>
        Search Results ({{ options.length }})
      </h4>
    </div>
    <div class="span6">
      <div class="guide">
        &nbsp;
      </div>
    </div>
  </div>
</div>

<div class="selector-list">
  <ul class="new-tree">
    {{#options}}
      <li class="tree-item business" {{ data 'option' }} data-id="{{ id }}">
        <div class="item-main">
          <div class="simple-link">
            <div class="row-fluid">
              <div class="span11">
                <div class="item-data">
                  <div class="tree-title-area">
                    {{{render '/static/mustache/people/popover.mustache' person=this}}}
                    <span class="url-link">
                      {{email}}
                    </span>
                  </div>
                </div>
              </div>
              <div class="span1">
                <input type="checkbox" rel="tooltip" title="map">
              </div>
            </div>
          </div>
        </div>
      </li>
    {{/options}}
  </ul>
</div>
