SCRIPTPATH=$( cd "$(dirname "$0")" ; pwd -P )
cd "${SCRIPTPATH}/../test"

source "${SCRIPTPATH}/init_test_env"

db_reset -d "ggrcdevtest"

echo -e "\nRunning access control integration tests"
nosetests integration.ggrc.access_control \
  --with-xunit \
  --logging-clear-handlers \
  -v ${@:1}
